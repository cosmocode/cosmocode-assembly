<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <name>CosmoCode Assembly</name>
  <artifactId>cosmocode-assembly</artifactId>
  <groupId>de.cosmocode</groupId>
  <version>1.1.0-SNAPSHOT</version>
  <packaging>jar</packaging>
  <scm>
    <developerConnection>scm:svn:https://commons.svn.cosmocode.de/cosmocode-assembly/trunk</developerConnection>
  </scm>
  <distributionManagement>
  <repository>
    <id>cosmocode-public-releases</id>
    <name>CosmoCode public releases</name>
    <url>scp://mvn.cosmocode.de:6589/var/www/mvn/</url>
  </repository>
  <snapshotRepository>
    <id>cosmocode-private-snapshots</id>
    <name>CosmoCode private snapshots</name>
    <url>http://mvn.cosmo/artifactory/libs-snapshots-local</url>
  </snapshotRepository>
  </distributionManagement>
  <profiles>
    <profile>
      <id>cosmocode-public-snapshots</id>
      <distributionManagement>
        <snapshotRepository>
          <id>cosmocode-public-snapshots</id>
          <name>CosmoCode public snapshots</name>
          <url>scp://mvn.cosmocode.de:6589/var/www/mvn/</url>
        </snapshotRepository>
      </distributionManagement>
    </profile>
  </profiles>
</project>
